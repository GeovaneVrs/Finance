{
  "expo": {
    "name": "Finance",
    "slug": "Finance",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Vector.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Vector.png",
      "resizeMode": "contain",
      "backgroundColor": "#00D09E"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Vector.png",
        "backgroundColor": "#00D09E"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/FinWise.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
